#!/usr/bin/env bash
# vocabularies for manipulating EXIT hooks
# 
# Author: Jaeho Shin <netj@sparcs.org>
# Created: 2011-12-24
################################################################################

bash_plugin_load() {
bash_unload_command=""
trap 'eval $bash_unload_command' EXIT
bash_add_unloader() {
    bash_unload_command="$bash_unload_command$1;"
}
}
